{
  "name": "expressjs-mvc",
  "version": "1.0.0",
  "description": "MVC Boilerplate for expressjs",
  "main": "./dist/server.js",
  "author": "fahriansyah",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist/ && babel ./src --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files",
    "dev": "npm run build && nodemon dist/server.js",
    "start": "npm run build && node dist/server.js",
    "fortmat": "npx prettier --write .",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "lint:check": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "mocha"
  },
  "dependencies": {
    "app-root-path": "^3.0.0",
    "bcrypt": "^5.0.1",
    "cloudinary": "^1.21.0",
    "crypto": "^1.0.1",
    "datauri": "^4.1.0",
    "dotenv": "^8.2.0",
    "eslint-plugin-node": "^11.1.0",
    "express": "^4.17.1",
    "express-validator": "^6.4.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.1",
    "module-alias": "^2.2.2",
    "multer": "^1.4.2",
    "mysql": "^2.18.1",
    "objection": "^1.6.11",
    "randomstring": "^1.1.5",
    "supertest": "^4.0.2",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "mocha": "^7.1.2",
    "prettier": "2.6.2",
    "rimraf": "^3.0.2"
  },
  "_moduleAliases": {
    "@root": ".",
    "@utils": "./app/Http/Utils"
  }
}
